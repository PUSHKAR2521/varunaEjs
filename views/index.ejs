      <script>
        const radios = document.getElementsByName("myRadio");
        const imageContainer = document.getElementById("image-container");
        const image = document.getElementById("image");

        // Set the initial image to none
        image.src = "images/body-img1.png";

        // Add event listeners to each radio button
        radios.forEach((radio) => {
          radio.addEventListener("click", () => {
            // Show the corresponding image when a radio button is clicked
            switch (radio.value) {
              case "option1":
                image.src = "images/body-img1.png";
                break;
              case "option2":
                image.src = "images/body-img2.png";
                break;
              case "option3":
                image.src = "images/body-img3.png";
                break;
              case "option4":
                image.src = "images/body-img4.png";
                break;
            }
          });
        });

        // Change the image every 5 seconds
        let currentImage = 0;
        const images = ["images/body-img1.png", "images/body-img2.png", "images/body-img3.png", "images/body-img4.png"];
        setInterval(() => {
          currentImage = (currentImage + 1) % images.length;
          image.src = images[currentImage];
          radios[currentImage].checked = true;
        }, 2000);
      </script>


