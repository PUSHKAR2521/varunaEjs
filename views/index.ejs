      <script>
        let count1 = 0; // starting number for counter1
        let count2 = 0; // starting number for counter2
        let count3 = 0; // starting number for counter3
        let count4 = 0; // starting number for counter4

        const counter1Element = document.getElementById("body-item1");
        const counter2Element = document.getElementById("body-item2");
        const counter3Element = document.getElementById("body-item3");
        const counter4Element = document.getElementById("body-item4");

        setInterval(function () {
          if (count1 < 1000) {
            count1 += 5; // increment by 1-5 every 1 second
            counter1Element.textContent = count1.toLocaleString() + "+" + " " + "Motors" + "\n" + "Pumps";
          }
        }, 15); // increment every 10 milliseconds

        setInterval(function () {
          if (count2 < 85) {
            count2 += 1; // increment by 1-5 every 1 second
            counter2Element.textContent = count2.toLocaleString() + " " + "Years of legacy";
          }
        }, 35); // increment every 10 milliseconds

        setInterval(function () {
          if (count3 < 55) {
            count3 += 1; // increment by 1-5 every 1 second
            counter3Element.textContent = count3.toLocaleString() + "+" + " " + "Countries";
          }
        }, 55); // increment every 10 milliseconds

        setInterval(function () {
          if (count4 < 3) {
            count4 += 1; // increment by 1-5 every 1 second
            counter4Element.textContent = count4.toLocaleString() + " " + "Pumps" + "\n" + "Produced per minutes";
          }
        }, 800); // increment every 10 milliseconds
      </script>




